<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.openjmsadapter.configuration.database">
  <class name="JmsDestinationParameters" table="jms_destination_parameters">
    <id column="id" name="id">
      <generator class="increment"/>
    </id>
    <property column="destinationName" name="destinationName"/>
    <property column="destinationType" name="destinationType"/>
    <property column="operationType" name="operationType"/>
    <property column="lastSequenceSent" name="lastSequenceSent"/>
    <property column="lastSequenceReceived" name="lastSequenceReceived"/>
  </class>
</hibernate-mapping>
